<%= form_tag checkout2_path do %>

  <%= label_tag(
    name = @rate_2day["service_name"] + @rate_2day["delivery_date"]
  ) %>
    <%= radio_button_tag(
      "price",
      (@rate_2day["total_price"]),
      checked = false)
    %>

  <%= label_tag(
    name = @rate_ground["service_name"] + @rate_ground["delivery_date"]
  ) %>
    <%= radio_button_tag(
      "price",
      (@rate_ground["total_price"]),
      checked = false)
    %>

  <%= label_tag(
    name = @rate_standard_on["service_name"] + @rate_standard_on["delivery_date"]
  ) %>
    <%= radio_button_tag(
      "price",
      (@rate_standard_on["total_price"]),
      checked = false)
    %>

    <%= submit_tag  %>
<% end %>
